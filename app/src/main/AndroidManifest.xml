<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maid.chifast">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_chifast"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round_chifast"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodApp">
        <activity
            android:name="com.maid.chifast.MainActivity"
            android:exported="false" />
        <activity android:name="com.maid.chifast.IntroActivity" android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>